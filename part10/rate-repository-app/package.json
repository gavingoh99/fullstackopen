{
  "name": "rate-repository-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint ./src/**/*.{ts,tsx} App.tsx --no-error-on-unmatched-pattern",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.5",
    "@expo/metro-config": "^0.3.18",
    "@react-native-async-storage/async-storage": "^1.17.5",
    "@react-native-picker/picker": "2.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "dotenv": "^16.0.1",
    "expo": "~44.0.0",
    "expo-linking": "~3.0.0",
    "expo-status-bar": "~1.2.0",
    "formik": "^2.2.9",
    "graphql": "^16.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-paper": "^4.12.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "use-debounce": "^8.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^27.5.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-plugin-jest": "^26.4.5",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^27.5.1",
    "jest-expo": "^45.0.1",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^28.0.3",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "react-devtools-core": "4.22.0"
  },
  "private": true
}
